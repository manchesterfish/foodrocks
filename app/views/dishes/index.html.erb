<h1>Listing dishes</h1>

<table>
  <thead>
    <tr>
      <th>Dish name</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dishes.each do |dish| %>
      <tr>
        <td><%= link_to image_tag(dish.ImageURL), dish %></td>
        <td><%= dish.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
